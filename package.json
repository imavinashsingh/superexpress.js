{
  "name": "super-express.js",
  "version": "0.2.0",
  "private": true,
  "author": {
    "name": "Avinash singh"
  },
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "babel-plugin-transform-decorators": "^6.24.1",
    "install": "^0.13.0",
    "jest": "^25.5.4",
    "npm": "^7.20.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./jest-setup-file.js"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "prod": "npm-run-all build server",
    "dev":"babel-node ./server/bin/www.js ",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest"
  }
}
